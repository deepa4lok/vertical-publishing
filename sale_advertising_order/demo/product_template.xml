<odoo>
    <record id="product_template_digital1" model="product.template">
        <field name="name">Product digital 1</field>
        <field name="categ_id" ref="product_category_ads_digital1" />
        <field name="list_price">42</field>
    </record>
    <record id="product_template_digital1_attribute_title" model="product.template.attribute.line">
        <field name="product_tmpl_id" ref="product_template_digital1"/>
        <field name="attribute_id" ref="attribute_title"/>
        <field name="value_ids" eval="[Command.link(ref('attribute_value_title1'))]"/>
    </record>
    <record id="product_template_digital2" model="product.template">
        <field name="name">Product digital 2 (multi issue)</field>
        <field name="categ_id" ref="product_category_ads_digital1" />
        <field name="list_price">42</field>
    </record>
    <record id="product_template_digital2_attribute_title" model="product.template.attribute.line">
        <field name="product_tmpl_id" ref="product_template_digital2"/>
        <field name="attribute_id" ref="attribute_title"/>
        <field name="value_ids" eval="[Command.link(ref('attribute_value_title2'))]"/>
    </record>
</odoo>
